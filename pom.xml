<?xml version="1.0" encoding="UTF-8"?>
<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd"
         xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <modelVersion>4.0.0</modelVersion>
    <groupId>io.nettapotek</groupId>
    <artifactId>reactor</artifactId>
    <version>1.0.1-SNAPSHOT</version>
    <packaging>pom</packaging>
    <name>Nettapotek lib</name>

    <modules>
        <module>meldingsdefinisjoner</module>
        <module>soap</module>
        <module>message-builder</module>
        <!--    <module>keycloak</module>-->
        <module>rest</module>
    </modules>
    <properties>
        <jackson.version>2.14.1</jackson.version>
        <maven.compiler.source>17</maven.compiler.source>
        <maven.compiler.target>17</maven.compiler.target>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>
        <version.bc>1.70</version.bc>
        <cxf.version>4.0.0</cxf.version>
        <version.jetty>11.0.13</version.jetty>
        <version.jjwt>0.9.1</version.jjwt>
        <version.nimbusds.jwt>9.25.6</version.nimbusds.jwt>
        <version.nimbusds.oauth2>10.4</version.nimbusds.oauth2>
        <version.slf4j>2.0.6</version.slf4j>
        <version.swagger>4.15.5</version.swagger>
        <version.testng>7.7.0</version.testng>
        <jakarta-xml-bind.version>4.0.0</jakarta-xml-bind.version>
        <jakarta-xml-soap.version>3.0.0</jakarta-xml-soap.version>
        <jakarta-xml-ws.version>4.0.0</jakarta-xml-ws.version>
        <glassfish-jaxb.version>4.0.1</glassfish-jaxb.version>
    </properties>

    <dependencyManagement>
        <dependencies>
            <dependency>
                <groupId>org.glassfish.jaxb</groupId>
                <artifactId>jaxb-bom</artifactId>
                <version>${glassfish-jaxb.version}</version>
                <type>pom</type>
                <scope>import</scope>
            </dependency>
            <dependency>
                <groupId>org.apache.cxf</groupId>
                <artifactId>cxf-bom</artifactId>
                <version>${cxf.version}</version>
                <type>pom</type>
                <scope>import</scope>
            </dependency>
        </dependencies>
    </dependencyManagement>

    <dependencies>
        <dependency>
            <groupId>jakarta.xml.bind</groupId>
            <artifactId>jakarta.xml.bind-api</artifactId>
            <version>${jakarta-xml-bind.version}</version>
        </dependency>
        <dependency>
            <groupId>jakarta.xml.soap</groupId>
            <artifactId>jakarta.xml.soap-api</artifactId>
            <version>${jakarta-xml-soap.version}</version>
        </dependency>
        <dependency>
            <groupId>jakarta.xml.ws</groupId>
            <artifactId>jakarta.xml.ws-api</artifactId>
            <version>${jakarta-xml-ws.version}</version>
        </dependency>
    </dependencies>
    <build>
        <pluginManagement>
            <plugins>
                <plugin>
                    <groupId>org.apache.cxf</groupId>
                    <artifactId>cxf-codegen-plugin</artifactId>
                    <version>4.0.0</version>
                </plugin>
            </plugins>
        </pluginManagement>
    </build>
</project>
